<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="465dp"
        android:layout_height="147dp"
        android:background="#03A9F4"
        android:text="INFORMASI SAYURAN"
        android:textAlignment="center"
        android:textSize="55dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="219dp"
        android:layout_height="206dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="12dp"
        android:background="#2196F3"
        app:layout_constraintEnd_toStartOf="@+id/imageButton7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/terong_4" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="213dp"
        android:layout_height="176dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="#2196F3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton"
        app:srcCompat="@drawable/tomat_5" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="208dp"
        android:layout_height="184dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="4dp"
        android:background="#2196F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.425"
        app:layout_constraintStart_toEndOf="@+id/imageButton3"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7"
        app:srcCompat="@drawable/bawang_2" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="213dp"
        android:layout_height="157dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="8dp"
        android:background="#2196F3"
        app:layout_constraintEnd_toStartOf="@+id/imageButton6"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton3"
        app:srcCompat="@drawable/selada_3" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="205dp"
        android:layout_height="160dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="4dp"
        android:background="#2196F3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButton5"
        app:layout_constraintTop_toBottomOf="@+id/imageButton4"
        app:srcCompat="@drawable/cabai_1" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="207dp"
        android:layout_height="205dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40sp"
        android:background="#2196F3"
        app:layout_constraintBottom_toTopOf="@+id/imageButton4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:srcCompat="@drawable/wortel_6" />


</androidx.constraintlayout.widget.ConstraintLayout>